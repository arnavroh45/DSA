COde: 
vector<int> ans;
    void solve(vector<int> adj[], vector<int> &vis, int i)
    {
        vis[i] = 1;
        ans.push_back(i);
        for(auto j : adj[i])
        {
            if(!vis[j])
            {
                solve(adj, vis, j);
            }
        }
    }
    vector<int> dfsOfGraph(int V, vector<int> adj[]) {
        // Code here
        vector<int> visited(V, 0);
        solve(adj, visited, 0);
        return ans;
        
    }
