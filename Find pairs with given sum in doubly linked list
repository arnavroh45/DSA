Code:
vector<pair<int, int>> findPairsWithGivenSum(Node *head, int target)
    {
        // code here
        Node* start = head;
        Node* end = head;
        vector<pair<int, int>> ans;
        while(end->next)
            end = end->next;
        while(start != end && start->prev != end)
        {
            if(start->data+end->data==target)
            {
                ans.push_back(make_pair(start->data, end->data));
                start = start->next;
                end = end->prev;
            }
            else if(start->data+end->data>target)
                end = end->prev;
            else
                start = start->next;
        }
        return ans;
    }
