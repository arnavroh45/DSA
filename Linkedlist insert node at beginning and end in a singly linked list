 Node *insertAtBegining(Node *head, int x) {
       // Your code here
       Node* root = new Node(x);
       root->data = x;
       root->next = head;
       return root;
    }
    
    
    //Function to insert a node at the end of the linked list.
    Node *insertAtEnd(Node *head, int x)  {
       // Your code here
       Node* temp = head;
       Node* newNodee = new Node(x);
       if(head==NULL)
        return newNodee;
       while(temp->next)
       {
           temp = temp->next;
       }
       temp->next = newNodee;
       return head;
    }
