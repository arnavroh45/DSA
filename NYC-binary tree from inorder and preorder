TreeNode* solve(int &index, vector<int>& preorder, vector<int>& inorder, int l, int r)
    {
        if(l>r)
            return NULL;
        TreeNode* root = new TreeNode(preorder[index]);
        int cut;
        for(int i = 0;i<inorder.size();i++)
        {
            if(inorder[i]==preorder[index])
            {
                cut = i;
                break;
            }

        }
        index++;
        root->left = solve(index, preorder, inorder, l, cut-1);
        root->right = solve(index, preorder, inorder, cut+1, r);
        return root;
    }
    TreeNode* buildTree(vector<int>& preorder, vector<int>& inorder) {
        int index = 0;
        return solve(index, preorder, inorder, 0, inorder.size()-1);
    }
