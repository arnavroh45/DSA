void reorderList(ListNode* head) {
        if(head==NULL || head->next==NULL || head->next->next==NULL)
            return;
        ListNode* curr = head;
        while(curr->next->next!=NULL)
        {
            curr = curr->next;
        }
        curr->next->next = head->next;
        head->next=curr->next;
        curr->next=NULL;
        reorderList(head->next->next);      
    }
